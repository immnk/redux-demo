{
  "name": "redux-demo",
  "version": "0.0.4",
  "description": "test redux functionality",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --config .eslintrc.js ",
    "build:watch": "webpack --progress --color --watch --display-error-details",
    "serve": "lite-server -c=bs-config.json",
    "build:prod": "webpack --mode=production --verbose --progress",
    "start": "npm i && concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "cross-env NODE_ENV=test npm run lint && nyc --reporter=lcov --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immnk/redux-demo.git"
  },
  "keywords": [
    "webpack",
    "module"
  ],
  "author": "Mani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/immnk/redux-demo/issues"
  },
  "homepage": "https://github.com/immnk/redux-demo#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "~7.3.4",
    "@babel/preset-env": "~7.3.4",
    "@babel/register": "~7.0.0",
    "babel-loader": "~8.0.5",
    "babel-plugin-istanbul": "~5.1.1",
    "babel-polyfill": "~6.26.0",
    "chai": "~4.2.0",
    "concurrently": "~4.1.0",
    "copy-webpack-plugin": "~5.0.0",
    "coveralls": "~3.1.0",
    "cross-env": "~5.2.0",
    "css-loader": "~2.1.0",
    "eslint": "~5.15.0",
    "file-loader": "~3.0.1",
    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~3.2.0",
    "lite-server": "~2.4.0",
    "mini-css-extract-plugin": "~0.5.0",
    "mocha": "~6.0.0",
    "node-sass": "~4.12.0",
    "nyc": "~13.3.0",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "require-hooks": "~0.0.6",
    "sass-loader": "~7.1.0",
    "terser-webpack-plugin": "~1.2.2",
    "url-loader": "~1.1.2",
    "webpack": "~4.32.0",
    "webpack-cli": "~3.2.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
